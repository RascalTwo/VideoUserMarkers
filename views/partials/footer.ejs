		</main>
		<footer class="flex justify-between py-1 pr-1 text-right border-t dark:text-gray-300 dark:bg-gray-900 border-slate-500">
		  <div class="theme-switch">
		    <input type="checkbox" class="theme-switch__input" id="theme-toggler" />
		    <label class="theme-switch__label" for="theme-toggler" aria-label="Switch to dark theme">
		      <span class="theme-switch__indicator"></span>
		      <span class="theme-switch__decoration"></span>
		    </label>
		  </div>
		  <script>
		    const themeSwitch = document.querySelector('#theme-toggler');
		    themeSwitch.addEventListener('change', function() {
		      const isDark = document.documentElement.classList.toggle('dark');
		      localStorage.setItem('r2Theme', isDark ? 'dark' : 'light');
		    });
		    themeSwitch.checked = !document.documentElement.classList.contains('dark');
		  </script>
