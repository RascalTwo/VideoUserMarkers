<%- await include("partials/header.ejs") %>
<%- await include("partials/sorting.ejs", { fields: { matchRatio: 'Total Match', title: 'Title', createdAt: 'Created', updatedAt: 'Updated' }, values: { search: query.search }}) %>
<div class="container px-4 mx-auto my-12 md:px-12">
  <div class="flex flex-wrap -mx-1 rounded-md shadow-md justify-evenly dark:shadow-slate-700 lg:-mx-4 bg-slate-300 dark:bg-slate-700">
    <% for (const match of sortArrayBy(matches, query.sort, query.descending)) { %>
    <%- match.type === 'collection' ? await include("partials/collection.ejs", { collection: match, search }) : await include("partials/marker.ejs", { marker: match, search }) %>
    <% } %>
  </div>
</div>
<%- await include("partials/footer.ejs") %>