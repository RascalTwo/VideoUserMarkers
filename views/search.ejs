<%- await include("partials/header.ejs") %>
<div class="container px-4 mx-auto my-12 md:px-12">
  <div class="flex flex-wrap -mx-1 lg:-mx-4">
    <% for (const match of matches) { %>
			<%- match.type === 'collection' ? await include("partials/collection.ejs", { collection: match, query }) : await include("partials/marker.ejs", { marker: match, query }) %>
    <% } %>
  </div>
</div>
<%- await include("partials/footer.ejs") %>