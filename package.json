{
  "name": "video-user-markers-backend",
  "version": "0.4.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently 'npm run build:watch' 'npm run start:watch'",
    "start": "node index.js",
    "start:watch": "nodemon --ignore ./public --ignore ./views index.js",
    "build": "npm run build:react && npm run build:tailwind",
    "build:react": "babel public/collection/*.jsx --out-dir public/collection",
    "build:tailwind": "tailwindcss -i ./index.css -o ./public/index.css",
    "build:watch": "concurrently 'npm run build:react -- --watch' 'npm run build:tailwind -- --watch'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "concurrently": "^7.4.0",
    "connect-mongo": "^4.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-flash": "^0.0.2",
    "express-promise-router": "^4.1.1",
    "express-session": "^1.17.3",
    "method-override": "^3.0.0",
    "mongoose": "^6.6.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-twitch-new": "^0.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "nodemon": "^2.0.20",
    "tailwindcss": "^3.2.4"
  }
}
